      <Label alignment="CENTER" layoutX="70.0" layoutY="264.0" prefHeight="38.0" prefWidth="261.0" text="RE-ENTER PASSWORD">
         <font>
            <Font size="17.0" />
         </font>
      </Label>
      <PasswordField fx:id="joinTxtRepassword" layoutX="114.0" layoutY="296.0" prefHeight="38.0" prefWidth="172.0" promptText="비밀번호를 입력하세요" />
   </children>
</AnchorPane>
